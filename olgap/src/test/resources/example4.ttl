# baseURI: http://inova8.com/intelligentgraph/example4/
# prefix: example4

@prefix : <http://inova8.com/intelligentgraph/example4/> .
@prefix example4: <http://inova8.com/intelligentgraph/example4/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix script: <http://inova8.com/script/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

example4:
  a owl:Ontology ;
.
example4:Person
  a owl:Class ;
  rdfs:label "Person" ;
.
example4:Peter
  a example4:Person ;
  example4:hasBMI "double height=_this.getFact(':hasHeight').doubleValue();  _this.getFact(':hasWeight').doubleValue()/(height*height)"^^script:groovy ;
  example4:hasBMIjs "var height=_this.getFact(':hasHeight').doubleValue();  _this.getFact(':hasWeight').doubleValue()/(height*height)"^^script:javascript ;
  example4:hasBMIpy "_this.getFact(':hasWeight').doubleValue()/( _this.getFact(':hasHeight').doubleValue()* _this.getFact(':hasHeight').doubleValue())"^^script:python ;
  example4:hasBMIpytest "height = _this.getFact(':hasHeight').doubleValue(); _result = _this.getFact(':hasWeight').doubleValue()/(height*height)"^^script:python ;
  example4:hasHeight 1.7 ;
  example4:hasWeight "62"^^xsd:decimal ;
.
example4:hasBMI
  a owl:DatatypeProperty ;
.
example4:hasHeight
  a owl:DatatypeProperty ;
.
example4:hasWeight
  a owl:DatatypeProperty ;
.
